{
  "name": "spd-web-app",
  "version": "0.0.1",
  "description": "Starter Project with ReactJS, Typescript, Webpack and Bootstrap",
  "main": "index.js",
  "homepage": "https://SearsHomeServices.github.io/spd-web-app/",
  "scripts": {
    "clean:all": "npm cache clean && npm run clean:dist && rimraf node_modules",
    "clean:dist": "rimraf dist && rimraf lint-reports",
    "build:dev": "npm run clean:dist && webpack --env=dev --progress --profile --colors --display-error-details --display-cached",
    "build:dev:ghpage": "npm run clean:dist && set deployenv=ghpage&&webpack --env=dev --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "npm run clean:dist && npm run test && webpack --env=prod --progress --profile --colors --display-error-details --display-cached",
    "start": "webpack-dev-server --env=dev --progress --profile --colors",
    "test": "jest --ci --runInBand",
    "test:update": "jest --ci --updateSnapshot --runInBand --detectOpenHandles",
    "deploy": "gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SearsHomeServices/spd-web-app.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "typescript",
    "webpack",
    "webpack2",
    "bootstrap",
    "less",
    "starter"
  ],
  "author": "",
  "license": "",
  "dependencies": {
    "@types/react": "^16.4.1",
    "@types/react-dom": "^16.0.6",
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-link-http": "^1.5.4",
    "autoprefixer": "^8.6.5",
    "bootstrap": "^4.1.1",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "postcss-loader": "^2.1.5",
    "react": "^16.4.1",
    "react-apollo": "^2.1.6",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^23.1.0",
    "jest-junit": "^5.1.0",
    "node-sass": "^4.9.0",
    "react-test-renderer": "^16.4.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-jest": "^22.4.6",
    "ts-loader": "^4.4.1",
    "ts-node": "^6.1.2",
    "tslint": "^5.10.0",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "browserslist": [
    "> 1%",
    "last 10 versions"
  ]
}
